<?xml version="1.0"?>
<openerp>
    <data>

        <!-- Menú configuración -->
        <menuitem 
            id="menu_cfd_mx" 
            name="Localización Mexico"
            parent="account.menu_finance_configuration"/>


        <!-- CATALOGO TIPORELACION -->
        <record id="tiporelacion_view_form" model="ir.ui.view">
            <field name="name">tiporelacion_view_form</field>
            <field name="model">cfd_mx.tiporelacion</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Catálogo Tipo Relacion">
                    <group colspan="4">
                        <field name="clave"/>
                        <field name="name"/>
                    </group>
                </form>
            </field>
        </record>
        <record id="tiporelacion_view_tree" model="ir.ui.view">
            <field name="name">tiporelacion_view_tree</field>
            <field name="model">cfd_mx.tiporelacion</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Catálogo Tipo Relacion">
                    <field name="clave"/>
                    <field name="name"/>
                </tree>
            </field>
        </record>
        <record id="tiporelacion_view_filter" model="ir.ui.view">
            <field name="name">tiporelacion_view.search</field>
            <field name="model">cfd_mx.tiporelacion</field>
            <field name="type">search</field>
            <field name="arch" type="xml">
                <search string="Catálogo Tipo Relacion">
                    <group>
                        <field name="name" string="Name" filter_domain="['|',('clave','ilike',self),('name','ilike',self)]"/>
                        <field name="clave"/>
                    </group>
                </search>
            </field>
        </record>
        <record id="action_tiporelacion" model="ir.actions.act_window">
            <field name="name">Catálogo Tipo Relacion</field>
            <field name="res_model">cfd_mx.tiporelacion</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
        <menuitem
            action="action_tiporelacion" 
            parent="menu_cfd_mx"
            id="menu_tiporelacion"
            sequence="100" />


