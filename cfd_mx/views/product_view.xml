<?xml version="1.0"?>
<openerp>
    <data>

        <record model="ir.ui.view" id="product_normal_form_cfdi_view">
            <field name="name">product_normal_form_cfdi_view</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="product.product_normal_form_view"/>
            <field name="arch" type="xml">
                <field name="name" position="after">
                    <field name="clave_prodser_id"/>
                    <field name="cuenta_predial"/>
                </field>
            </field>
        </record>

        <record model="ir.ui.view" id="product_uom_form_cfdi_view">
            <field name="name">product_uom_form_cfdi_view</field>
            <field name="model">product.uom</field>
            <field name="inherit_id" ref="product.product_uom_form_view"/>
            <field name="arch" type="xml">
                <field name="category_id" position="after">
                    <field name="clave_unidadesmedida_id"/>
                </field>
            </field>
        </record>

    <!--   -->
    </data>
</openerp>